

GRANT ALL PRIVILEGES ON grafana.* to '<%= node['grafana']['mysql_user'] %>'@'localhost' identified by '<%= node['grafana']['mysql_password'] %>';


CREATE TABLE `session` (
    `key`   char(16) not null,
    `data`  blob,
    `expiry` int(11) unsigned not null,
    primary key (`key`)
) ENGINE=ndbcluster default charset=utf8;

